		<provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>

		<class>com.doubleclue.dcem.userportal.entities.ApplicationHubEntity</class>
		<class>com.doubleclue.dcem.userportal.entities.KeepassEntryEntity</class>
		<class>com.doubleclue.dcem.core.entities.DcemGroup</class>
		<class>com.doubleclue.dcem.core.entities.DcemRole</class>
		<class>com.doubleclue.dcem.core.entities.DomainEntity</class>
		<class>com.doubleclue.dcem.core.entities.DcemUserExtension</class>
		<class>com.doubleclue.dcem.core.entities.DcemUser</class>
		<class>com.doubleclue.dcem.core.entities.DcemAction</class>		
		<class>com.doubleclue.dcem.core.entities.DomainConfig</class>
		<class>com.doubleclue.dcem.userportal.entities.DbJsonConverterAppHub</class>	
		
		<properties>
			<property name="hibernate.show_sql" value="false" />
		</properties>
	</persistence-unit>
</persistence>
